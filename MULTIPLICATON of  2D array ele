#include <bits/stdc++.h>
  using namespace std;
  
  int main()
  {
    int t;
    cin>>t;
    while(t--){
    
    int n, i , pro = 1 ;
    cin>> n;
    
    // storing numbers in array
    int a[n];
    for( i = 1 ; i<=n; i++)
    cin>>a[i];
    
    //for accessing each number of array
    for( i =1; i<=n; i++)
    pro = a[i]*pro;
    cout<<pro<<endl;
    }
    return 0;
  }
